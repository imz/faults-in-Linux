}
*/

pattern rcu_lock {
 file = "rcu_lock.cocci"
 flags = "-no_includes -include_headers -D rcu_read_lock=rcu_read_lock -D rcu_read_unlock=rcu_read_unlock"
 correl = strict
}

pattern srcu_lock {
 file = "rcu_lock.cocci"
